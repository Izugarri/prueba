<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
      <rewrite>
          <rules>
              <rule name="Root Hit Redirect" stopProcessing="true">
                  <match url="https://simplyred.es" />
                  <action type="Redirect" url="https://www.simplyred.es" />
              </rule>
          </rules>
      </rewrite>
  </system.webServer>
</configuration>
